    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "express-enrouten": "^1.3.0",
    "express-validator": "^4.3.0",
    "firebase": "^5.7.2",
    "firebase-admin": "^6.4.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mailer": "^0.6.7",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "multer": "^1.3.1",
    "mysql2": "^1.6.1",
    "nconf": "^0.10.0",
