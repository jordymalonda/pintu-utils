{
  "name": "pintu-scrapper",
  "version": "1.0.0",
  "description": "Pintu Scrapper API",
  "scripts": {
    "start": "node app.js",
    "start:dev": "npm-run-all --parallel security-check start lint:watch",
    "start:share": "npm-run-all --parallel start:dev localtunnel",
    "security-check": "nsp check",
    "localtunnel": "lt --subdomain boostapi --port 3000",
    "lint": "esw --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress \"tests/*.js\" --exit",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Starter",
    "Kit"
  ],
  "author": "Jordy Malonda",
  "license": "MIT",
  "dependencies": {
